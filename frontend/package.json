{
  "name": "fashion-supplychain-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "npm@11.7.0",
  "scripts": {
    "dev": "NODE_OPTIONS=--experimental-global-webcrypto vite",
    "dev:host": "NODE_OPTIONS=--experimental-global-webcrypto vite --host 0.0.0.0 --port 5173",
    "dev:all": "concurrently -k -n backend,frontend -c blue,green \"bash -lc 'set -a; [ -f ../.run/backend.env ] && . ../.run/backend.env; set +a; cd ../backend && mvn -q spring-boot:run'\" \"npm run dev:host\"",
    "build": "NODE_OPTIONS=--experimental-global-webcrypto vite build",
    "build:strict": "NODE_OPTIONS=--experimental-global-webcrypto tsc && NODE_OPTIONS=--experimental-global-webcrypto vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --cache --cache-location node_modules/.cache/eslint --cache-strategy content",
    "lint:fix": "eslint . --ext ts,tsx --fix --cache --cache-location node_modules/.cache/eslint --cache-strategy content",
    "type-check": "tsc --noEmit --incremental --tsBuildInfoFile node_modules/.cache/tsbuildinfo",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json,md}\" \"scripts/**/*.{mjs,js,json}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json,md}\" \"scripts/**/*.{mjs,js,json}\"",
    "test": "NODE_OPTIONS=--experimental-global-webcrypto vitest run",
    "test:watch": "NODE_OPTIONS=--experimental-global-webcrypto vitest",
    "quality:score": "node ./scripts/qualityScore.mjs",
    "check:unused": "ts-prune",
    "check:deps": "depcheck",
    "check:circular": "madge --circular --extensions ts,tsx src/",
    "check:all": "npm run lint && npm run type-check && npm run check:deps && npm run check:circular",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/charts": "^2.6.7",
    "@ant-design/icons": "^6.1.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@types/qrcode": "^1.5.6",
    "antd": "^6.1.3",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.6",
    "exceljs": "^4.4.0",
    "lottie-react": "^2.4.1",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "three": "^0.182.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.1.7",
    "jscpd": "^4.0.8",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "terser": "^5.36.0",
    "ts-prune": "^0.10.3",
    "typescript": "5.3.3",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}
